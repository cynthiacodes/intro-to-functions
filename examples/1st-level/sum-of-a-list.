"use strict"

function sumList(numbers) {

    let runningTotal = 0;

    for (let eachNumber of numbers) {
        runningTotal++;
    }
    return runningTotal
}



console.log(sumList([3, 10, 1]), 'should be 14')
sumList([3, 10, 1, -4]) should be 10
sumList([0, 0, 0, 1]) should be 1
sumList([0, 0]) should be 0